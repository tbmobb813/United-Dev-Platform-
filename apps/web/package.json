{
  "name": "@udp/web",
  "version": "0.0.2",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "db:backup": "pg_dump $DATABASE_URL > backup-$(date +%Y%m%d-%H%M%S).sql",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@monaco-editor/react": "^4.6.0",
    "@prisma/client": "^6.16.2",
    "@tanstack/react-query": "^5.51.0",
    "@udp/ai": "workspace:^",
    "@udp/config": "workspace:^",
    "@udp/editor-core": "workspace:^",
    "@udp/ui": "workspace:^",
    "monaco-editor": "^0.49.0",
    "next": "14.2.32",
    "next-auth": "^4.24.11",
    "prisma": "^6.16.2",
    "qrcode.react": "^3.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "uuid": "^9.0.0",
    "y-protocols": "^1.0.6",
    "y-websocket": "^1.5.0",
    "yjs": "^13.6.27"
  },
  "devDependencies": {
    "@types/react": "18.2.79",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "@udp/eslint-config": "workspace:*",
    "@udp/prettier-config": "workspace:*",
    "eslint": "^9.36.0",
    "eslint-config-next": "^14.2.5",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "extends": [
      "@udp/eslint-config/react"
    ],
    "rules": {
      "@next/next/no-duplicate-head": "off"
    }
  },
  "prettier": "@udp/prettier-config"
}
